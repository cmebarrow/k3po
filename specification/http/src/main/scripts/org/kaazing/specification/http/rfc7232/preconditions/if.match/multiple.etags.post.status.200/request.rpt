connect http://localhost:8000/index.html
connected

write method "GET"
write version "HTTP/1.1"
write header host
write header content-length
write flush

read status "200" /.*/
read header "ETag" /(?<entityTag>.+)/
read notify READ_ETAG

connect await READ_ETAG
connect http://localhost:8000/index.html
connected

write method "POST"
write version "HTTP/1.1"
write header host
write header "If-Match" ${entityTag}, "\"tag1\"", "\"tag2\""
write header content-length
write flush

read status "200" /.*/
